					(Field) arguments.get(1), (SoccerPitch) arguments.get(2));
		case RulesPackage.REGULAR_FIELDS___PERFORM_FWD__ISAPPLICABLEMATCH:
			return perform_FWD((IsApplicableMatch) arguments.get(0));
		case RulesPackage.REGULAR_FIELDS___CHECK_TYPES_FWD__MATCH:
			return checkTypes_FWD((Match) arguments.get(0));
		case RulesPackage.REGULAR_FIELDS___IS_APPROPRIATE_FWD_FIELD_5__FIELD:
			return isAppropriate_FWD_Field_5((Field) arguments.get(0));
		case RulesPackage.REGULAR_FIELDS___IS_APPROPRIATE_FWD_SOCCER_PITCH_2__SOCCERPITCH:
			return isAppropriate_FWD_SoccerPitch_2((SoccerPitch) arguments
					.get(0));
		}
		return super.eInvoke(operationID, arguments);
	}

} //RegularFieldsImpl
