			return perform_FWD((IsApplicableMatch) arguments.get(0));
		case RulesPackage.GOAL_FIELDS___CHECK_TYPES_FWD__MATCH:
			return checkTypes_FWD((Match) arguments.get(0));
		case RulesPackage.GOAL_FIELDS___IS_APPROPRIATE_FWD_SOCCER_PITCH_5__SOCCERPITCH:
			return isAppropriate_FWD_SoccerPitch_5((SoccerPitch) arguments
					.get(0));
		case RulesPackage.GOAL_FIELDS___IS_APPROPRIATE_FWD_GOAL_FIELD_1__GOALFIELD:
			return isAppropriate_FWD_GoalField_1((GoalField) arguments.get(0));
		}
		return super.eInvoke(operationID, arguments);
	}

} //GoalFieldsImpl
